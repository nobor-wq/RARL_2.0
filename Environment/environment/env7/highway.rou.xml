<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- 定义车辆类型 -->
    <vType id="Auto" accel="7.6" decel="7.6" 
           sigma="0.000" maxSpeed="30.0" length="5.0" color="1,0,0" vClass="passenger" 
           minGap="1.0" tau="1.0" laneChangeModel="LC2013"/>
    
    <vType id="Car" accel="1.0" decel="1.6" sigma="0.1" maxSpeed="22.0" length="5.0"
           color="0,1,0" vClass="passenger" minGap="1.0" tau="1.0" laneChangeModel="LC2013"/>
    
    <vType id="Bus" accel="2.0" decel="2.0" sigma="0.01" maxSpeed="18.0" length="10.0"
           color="0,1,1" vClass="bus" minGap="1.0" tau="1.0" laneChangeModel="LC2013"/>
    
    <vType id="Truck" accel="1.5" decel="3.0" sigma="0.1" maxSpeed="15.0" length="15.0"
           color="1,1,1" vClass="truck" minGap="1.0" tau="1.0" laneChangeModel="LC2013"/>

    <vType id="Car_NoLaneChange" accel="1.0" decel="1.6" sigma="0.1" maxSpeed="18.0" length="5.0"
           color="0,1,0" vClass="passenger" minGap="1.0" tau="1.0" laneChangeModel="LC2013" lcStrategic="-1"/>

    <vType id="Bus_NoLaneChange" accel="2.0" decel="2.0" sigma="0.01" maxSpeed="15.0" length="10.0"
           color="0,1,1" vClass="bus" minGap="1.0" tau="1.0" laneChangeModel="LC2013" lcStrategic="-1"/>

    <vType id="Truck_NoLaneChange" accel="1.5" decel="3.0" sigma="0.1" maxSpeed="13.0" length="15.0"
           color="1,1,1" vClass="truck" minGap="1.0" tau="1.0" laneChangeModel="LC2013" lcStrategic="-1"/>

    
    <!-- 注入车辆流 -->
    <flow id="f_0" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Car_NoLaneChange">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 20)" />
    </flow>

    <flow id="f_1" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Bus_NoLaneChange">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 18)" />
    </flow>

    <flow id="f_2" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Truck_NoLaneChange">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 15)" />
    </flow>

    <flow id="f_3" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Car">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 20)" />
    </flow>

    <flow id="f_4" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Bus">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 18)" />
    </flow>

    <flow id="f_5" begin="0.00" from="E18" to="E18" end="220" probability="0.7" type="Truck">
        <param key="departLane" value="random" />
        <param key="departSpeed" value="uniform(10, 15)" />
    </flow>


    <!-- 定义路线 -->
    <route id="navigation" edges="E18"/>
    <!-- 手动添加车辆 -->
    <vehicle id="Auto_vehicle" color="1,0,0" departLane="1" depart="100" type="Auto" route="navigation"/>
</routes>
